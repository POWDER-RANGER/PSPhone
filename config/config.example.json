{
  "display": {
    "resolution": {
      "width": 1920,
      "height": 1080,
      "fps": 60
    },
    "bitrate": {
      "video": 15000,
      "audio": 320
    },
    "codec": {
      "video": "H.264",
      "audio": "AAC"
    }
  },
  "transport": {
    "type": "wifi",
    "protocol": "tcp",
    "port": 9295,
    "encryption": true,
    "compression": true
  },
  "controller": {
    "mapping": {
      "dualshock4": true,
      "dualsense": true,
      "touchEnabled": true,
      "gyroEnabled": true
    },
    "haptics": {
      "enabled": true,
      "intensity": 0.8
    },
    "adaptiveTriggers": {
      "enabled": true,
      "sensitivity": 0.75
    }
  },
  "accessibility": {
    "highContrast": false,
    "audioDescription": false,
    "subtitles": false,
    "buttonRemapping": true
  },
  "sony": {
    "complianceMode": "strict",
    "thirdPartyCode": false,
    "officialSDKOnly": true
  }
}
